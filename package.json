{
  "name": "generate-nft-metadata",
  "version": "1.4.0",
  "description": "Simple toolset to generate metadata",
  "repository": {
    "type": "git",
    "url": "https://github.com/fransyozef/generate-nft-metadata"
  },
  "main": "index.js",
  "bin": "index.js",
  "pkg": {
    "assets": [
      "assets/**/*",
      "node_modules/**/*",
      "src/**/*"
    ]
  },
  "scripts": {
    "start": "npm run build",
    "build": "node index.js",
    "clean:build": "rm -Rf build",
    "clean:package": "rm package-lock.json",
    "clean:node_modules": "rm -Rf node_modules",
    "clean": "npm run clean:build && npm run clean:package && npm run clean:node_modules",
    "rebuild": "npm run clean:build && npm start",
    "reinstall": "npm run clean && npm install",
    "tool4": "node utils/tool4.js",
    "tool6": "node utils/tool6.js",
    "tool1": "npm run clean:build && node utils/tool1.js",
    "tool3": "npm run clean:build && node utils/tool3.js",
    "generate:fromImages": "npm run clean:build && node utils/generateFromImages.js",
    "tool5": "npm run clean:build && node utils/tool5.js",
    "generate:combinedMetadataJson": "npm run clean:build && node utils/combinedMetadataJson.js",
    "generate:importTraits": "npm run clean:build && node utils/importTraits.js"
  },
  "author": "Fransjo Leihitu (fransyozef)",
  "license": "MIT",
  "dependencies": {
    "canvas": "^2.9.1",
    "csv-parse": "^5.0.4",
    "mime-types": "^2.1.35",
    "sha1": "^1.1.1"
  }
}
